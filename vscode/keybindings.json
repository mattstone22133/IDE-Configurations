// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+alt+i",
        "command": "macros.override_up"
    },
    {
        "key": "ctrl+alt+k",
        "command": "macros.override_down"
    },
    {
        "key": "ctrl+alt+j",
        "command": "macros.override_leftword"
    },
    {
        "key": "ctrl+alt+l",
        "command": "macros.override_rightword"
    },
    {
        "key": "alt+i",
        "command": "macros.override_up"
    },
    {
        "key": "alt+k",
        "command": "macros.override_down"
    },
    {
        "key": "alt+j",
        "command": "macros.override_left"
    },
    {
        "key": "alt+l",
        "command": "macros.override_right"
    },
    {
        "key": "shift+alt+i",
        "command": "macros.override_upselect"
    },
    {
        "key": "shift+alt+k",
        "command": "macros.override_downselect"
    },
    {
        "key": "shift+alt+j",
        "command": "macros.override_leftselect"
    },
    {
        "key": "shift+alt+l",
        "command": "macros.override_rightselect"
    },
    {
        "key": "shif+ctrl+alt+j",
        "command": "macros.override_leftselect"
    },
    {
        "key": "shift+ctrl+alt+l",
        "command": "macros.override_rightwordselect"
    },
    {
        "key": "shift+ctrl+alt+j",
        "command": "macros.override_leftwordselect"
    },
    {
        "key": "shift+ctrl+alt+k",
        "command": "macros.override_downselect"
    },
    {
        "key": "shift+ctrl+alt+i",
        "command": "macros.override_upselect"
    },


    {
        "key": "shift+ctrl+alt+o",
        "command": "macros.override_endselect"
    },
    {
        "key": "shift+ctrl+alt+u",
        "command": "macros.override_homeselect"
    },
    {
        "key": "ctrl+alt+o",
        "command": "macros.override_end"
    },
    {
        "key": "ctrl+alt+u",
        "command": "macros.override_home"
    },
    {
        "key": "shift+ctrl+alt+/",
        "command": "macros.override_showinexplorer"
    },
    {
        "key": "ctrl+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+k",
        "command": "-editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+f",
        "command": "editor.action.formatSelection",
        "when": "editorHasSelection && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+f",
        "command": "-editor.action.formatSelection",
        "when": "editorHasSelection && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k s",
        "command": "-workbench.action.files.saveAll"
    },
    {
        "key": "ctrl+shift+s",
        "command": "workbench.action.files.saveAll"
    },
    {
        "key": "ctrl+shift+s",
        "command": "-workbench.action.files.saveAs"
    },

    {
        "key": "ctrl+shift+k",
        "command": "macros.override_pageDown"
    },
    {
        "key": "ctrl+shift+i",
        "command": "macros.override_pageUp"
    },
    {
        "key": "alt+shift+r",
        "command": "macros.override_renameSymbol"
    },
    {
        "key": "alt+shift+f",
        "command": "macros.override_find_all_references"
    },
    {
        "key": "ctrl+alt+shift+f",
        "command": "macros.override_find_in_files"
    },
    {
        "key": "ctrl+alt+shift+y",
        "command": "macros.override_back"
    },
    {
        "key": "ctrl+alt+shift+p",
        "command": "macros.override_forward"
    },
    
    //debugging
    {
        "key": "ctrl+alt+w",
        "command": "macros.override_debugger_continue"
    },
    {
        "key": "ctrl+alt+s",
        "command": "macros.override_debugger_step"
    },
    {
        "key": "ctrl+alt+d",
        "command": "macros.override_debugger_stepinto"
    },
    {
        "key": "ctrl+alt+a",
        "command": "macros.override_debugger_stepout"
    },
    {
        "key": "ctrl+r",
        "command": "macros.override_debugger_startexecution"
    },
    {
        "key": "ctrl+f2",
        "command": "macros.override_debugger_stopexecution"
    },
    {
        "key" : "shift+ctrl+b",
        "command":"macros.override_debugger_togglebreakpoint"
    },

    //code navigation
    {
        "key" : "shift+alt+s",
        "command":"macros.override_find_symbols"
    },
    {
        "key" : "alt+m",
        "command":"macros.override_go_to_symbol"
    },
    {
        "key" : "alt+ctrl+e",
        "command":"macros.override_go_to_definition"
    },
    {
        "key" : "alt+g",
        "command":"macros.override_go_to_definition"
    },
    {
        "key" : "shift+ctrl+alt+e",
        "command":"macros.override_peak_definition"
    },
    {
        "key" : "shift+alt+o",
        "command":"macros.override_quick_open_file"
    },
    
]